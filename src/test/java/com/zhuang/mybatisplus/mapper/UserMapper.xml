<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhuang.mybatisplus.mapper.UserMapper">
    <select id="getCurrentUser" resultType="com.zhuang.mybatisplus.entity.User">
        select *
        from sys_user
        where id = '@env{user.userId}'
        and @permission{user:query:all?1=2}
    </select>
</mapper>
